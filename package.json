{
  "name": "anime-app",
  "author": "Luiz <doginuwu@gmail.com>",
  "version": "1.0.0",
  "description": "An Electron boilerplate including TypeScript, React, Jest and ESLint.",
  "main": ".webpack/main",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-forge publish",
    "lint": "eslint . --ext js,ts",
    "test": "jest"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "electron-acrylic-window": "^0.5.11",
    "electron-store": "^8.1.0",
    "fluid-player": "^3.18.0",
    "phosphor-react": "^1.4.1",
    "plyr-react": "^5.3.0",
    "postcss": "^8.4.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-loader": "4.13.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-player": "^2.12.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.14.1",
    "react-wavify": "^1.8.3",
    "styled-components": "6.0.3",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.22.8",
    "@babel/plugin-transform-runtime": "7.22.7",
    "@babel/preset-env": "7.22.7",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@electron-forge/cli": "6.2.1",
    "@electron-forge/maker-deb": "6.2.1",
    "@electron-forge/maker-rpm": "6.2.1",
    "@electron-forge/maker-squirrel": "6.2.1",
    "@electron-forge/maker-zip": "6.2.1",
    "@electron-forge/plugin-webpack": "6.2.1",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@types/electron-devtools-installer": "2.2.2",
    "@types/jest": "29.5.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "astroturf": "^1.2.0",
    "babel-loader": "9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "19.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-standard": "5.0.0",
    "file-loader": "^6.2.0",
    "jest": "29.6.1",
    "npm-run-all": "4.1.5",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.0",
    "style-loader": "^3.3.3",
    "svg-url-loader": "^8.0.0",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "5.1.6",
    "wait-on": "7.0.1"
  },
  "overrides": {

  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "anime-app",
        "executableName": "anime-app"
      },
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack/main.webpack.js",
            "renderer": {
              "config": "./webpack/renderer.webpack.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./electron/bridge.ts"
                  }
                }
              ]
            }
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "anime-app",
            "setupExe": "anime-app.exe",
            "noMsi": true,
            "icon": "assets/favicon.ico",
            "setupIcon": "assets/favicon.ico"
          }
        }
      ]
    }
  }
}
