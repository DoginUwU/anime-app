{
  "name": "anime-app",
  "author": "Luiz <doginuwu@gmail.com>",
  "version": "2.0.0",
  "description": "An Electron boilerplate including TypeScript, React, Jest and ESLint.",
  "main": ".webpack/main",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-forge publish",
    "lint": "eslint . --ext js,ts",
    "test": "jest"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "electron-acrylic-window": "^0.5.11",
    "fluid-player": "^3.0.4",
    "phosphor-react": "^1.4.1",
    "plyr-react": "^5.0.2",
    "postcss": "^8.4.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "react-player": "^2.10.1",
    "react-router-dom": "^6.3.0",
    "styled-components": "5.3.5",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/plugin-transform-runtime": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
    "@electron-forge/cli": "6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-wix": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@types/electron-devtools-installer": "2.2.2",
    "@types/jest": "28.1.1",
    "@types/react": "17.0.11",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "astroturf": "^1.0.1",
    "babel-loader": "8.0.6",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "19.0.3",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-standard": "5.0.0",
    "file-loader": "^6.2.0",
    "jest": "28.1.1",
    "npm-run-all": "4.1.5",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "ts-jest": "28.0.4",
    "ts-loader": "^9.3.0",
    "typescript": "4.7.3",
    "wait-on": "6.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "anime-app",
        "executableName": "anime-app"
      },
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack/main.webpack.js",
            "renderer": {
              "config": "./webpack/renderer.webpack.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./electron/bridge.ts"
                  }
                }
              ]
            }
          }
        ]
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "anime-app"
          }
        }
      ]
    }
  }
}
